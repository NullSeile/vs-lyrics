{
  "name": "vs-lyrics",
  "displayName": "vs-lyrics",
  "description": "This extension will show lyrics of the song that is currently playing on Spotify.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.78.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NullSeile/vs-lyrics"
  },
  "icon": "assets/images/icon.png",
  "__metadata": {
    "publisherDisplayName": "Lia Bertran"
  },
  "publisher": "nullptr",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "vs-lyrics.openview",
          "name": "VS Lyrics"
        }
      ]
    },
    "commands": [
      {
        "command": "vs-lyrics.authorize",
        "title": "Authorize",
        "category": "VS Lyrics"
      },
      {
        "command": "vs-lyrics.logout",
        "title": "Logout",
        "category": "VS Lyrics"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.78.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.7.7",
    "express": "^4.19.2"
  }
}
